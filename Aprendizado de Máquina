Entrega 1: Exploração de Dados e Pré-processamento

import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns

from google.colab import files
uploaded = files.upload()

import io
df = pd.read_csv(io.BytesIO(uploaded['luna_pets.csv']))

print(df.head())
print(df.info())
print(df.describe())

df.dropna(inplace=True)

sns.pairplot(df)
plt.show()

df_encoded = pd.get_dummies(df, columns=['product_id'])

df_encoded.to_csv('dados_processados.csv', index=False)
files.download('dados_processados.csv')

